<!doctype html>
<html lang="en">
  <head>
    <title>{{ properties.title }}</title>
    <meta charset="utf-8">
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0 maximum-scale=1, user-scalable=no"
    >
    <link
      rel="stylesheet"
      href="{{ bsi_cx_css_href() }}"
    >
  </head>
  <body>
    <main data-bsi-dropzone="content">
      {% block main %}{% endblock %}
    </main>

    <script
      src="{{ bsi_cx_js_module_runtime_href() }}"
      defer="defer"
    ></script>
    <script
      src="{{ bsi_cx_js_module_href('main') }}"
      defer="defer"
    ></script>
    {{ bsi_cx_js_module_missing_chunks_import() }}
  </body>
</html>
