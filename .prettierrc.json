{
    "tabWidth": 2,
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "useTabs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "twigSingleQuote": true,
    "embeddedSingleQuote": true,
    "htmlWhitespaceSensitivity": "css",
    "singleLineLinkTags": false,
    "indentSchema": false,
    "plugins": [
        "@destination/prettier-plugin-twig"
    ]
}